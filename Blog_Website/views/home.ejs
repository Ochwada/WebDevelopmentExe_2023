<%- include("partials/header"); -%>

<h1>Home</h1>

<p><%= homeStartingContent %></p>

<div class="row">
    <div class="">
        <% posts.forEach( (post) => { %>
        <h2><%= post.title %></h2>
        <p>
            <%= post.content.substr(0, 150) + " ...";%> 
            <a href="/posts/<%=post.title%> "> Read More </a>
        </p>
        <% }); %>
    </div>
</div>

<%- include("partials/footer"); -%>