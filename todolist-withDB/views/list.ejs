<%- include("header") -%>

<div class="box" id="heading">
  <h1> <%= listTitle %> </h1>
</div>
<div class="box">


   <% newListItems.forEach((item) =>{%>
  <div class="item">
    <input type="checkbox" />
    <p><%=item.name %></p>
  </div>
   <% })%>


  <form class="item" action="/" method="post">
    <input type="text" placeholder="New Item" name="newItem" autocomplete="off" />
    <button type="submit" name="list" value=<%"listTitle" %>>+</button>
  </form>
</div>

<%- include("footer") -%>